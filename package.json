    "chokidar": "^3.0.2",
    "eslint": "^6.4.0",
    "husky": "^3.0.6",
    "jest": "^24.9.0",
    "ts-jest": "^24.1.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3"
  },
  "files": [
    "dist",
    "types"
  ],
