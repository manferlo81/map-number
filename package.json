  "keywords": [
    "processing",
    "p5",
    "p5.js",
    "map",
    "floor",
    "round",
    "limit",
    "number",
    "numbers"
  ],
  "main": "dist/map.js",
  "module": "dist/map.mjs",
  "browser": "dist/map.umd.js",
  "types": "types/index.d.ts",
  "typings": "types/index.d.ts",
  "scripts": {
    "clean": "rimraf dist types coverage node_modules/.cache/bundlib node_modules/.cache/jest",
