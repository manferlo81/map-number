{
  "name": "map-number",
  "version": "1.2.5",
  "description": "processing/p5.js map like function, including floating point numbers support",
  "main": "dist/map.cjs.js",
  "module": "dist/map.es.js",
  "browser": "dist/map.umd.js",
  "bundlib": {
    "name": "mapNum",
    "min": "browser"
  },
  "types": "types/index.d.ts",
  "typings": "types/index.d.ts",
  "keywords": [
    "processing",
    "p5",
    "p5.js",
    "map",
    "floor",
    "round",
    "limit",
    "number",
    "numbers"
  ],
  "scripts": {
    "clean": "rm -rf node_modules/.cache/bundlib node_modules/.cache/jest dist types coverage",
    "lint": "tslint src/**/*.ts",
