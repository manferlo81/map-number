{
  "name": "map-number",
  "version": "1.2.6",
  "description": "processing/p5.js map like function, including floating point numbers support",
  "main": "dist/map.cjs.js",
  "module": "dist/map.es.js",
  "browser": "dist/map.umd.js",
  "bundlib": {
    "name": "mapNum",
    "min": "browser"
  },
  "types": "types/index.d.ts",
  "typings": "types/index.d.ts",
  "keywords": [
    "processing",
    "p5",
    "p5.js",
    "map",
    "floor",
    "round",
    "limit",
    "number",
    "numbers"
  ],
  "scripts": {
    "clean": "rimraf dist types coverage node_modules/.cache/bundlib node_modules/.cache/jest",
    "lint": "eslint src/** __test__/**",
    "dev": "bundlib -dw",
    "build": "bundlib -d",
    "test": "jest --color -w 3",
    "prerelease": "npm run clean && npm run lint && npm test && npm run build",
    "release": "standard-version"
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.12",
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "@typescript-eslint/parser": "^2.8.0",
    "bundlib": "^0.13.4",
    "chokidar": "^3.3.0",
    "eslint": "^6.7.0",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "rimraf": "^3.0.0",
    "standard-version": "^7.0.1",
    "ts-jest": "^24.2.0",
    "typescript": "^3.7.2"
  },
  "files": [
    "dist",
    "types"
  ],
  "author": {
    "name": "Manuel Fern√°ndez",
    "email": "manferlo81@gmail.com"
  },
  "repository": {
    "type": "git",
